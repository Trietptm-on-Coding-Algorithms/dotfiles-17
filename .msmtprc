# MSMTP Configuration File

account lists
  logfile ~/var/log/msmtp.log
  host smtp.gmail.com
  from jimmy.tty@gmail.com
  tls on
  tls_starttls off
  tls_certcheck off
  auth login
  user jimmy.tty@gmail.com

account opmbx
  logfile ~/var/log/msmtp.log
  host smtp.openmailbox.org
  from jimmy.tty@openmailbox.org
  tls on
  tls_starttls off
  tls_certcheck off
  auth login
  user jimmy.tty@openmailbox.org

# Set a default account
# account default : default
